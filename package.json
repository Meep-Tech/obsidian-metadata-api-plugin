{
	"name": "obsidian-metadata-api-plugin",
	"version": "1.0.0",
	"description": "This plugin provides a global js api for accessing and updating frontmatter and other note metadatas.",
	"main": "build/plugin/main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lib": "tsc -skipLibCheck -p \"tsconfig-lib.json\" && node esbuild.config.mjs library",
		"test": "npx jest",
		"bdd": "npx jest -i --watch --no-cache",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"MetaScry",
		"Scrier",
		"Sections",
		"Headings",
		"Cache",
		"CodeBlocks",
		"Code",
		"JSX",
		"Metadata",
		"Obsidian",
		"Api",
		"Plugin",
		"JS",
		"Scripts",
		"Script",
		"Scripting",
		"DB",
		"Data",
		"Update",
		"Frontmatter",
		"Sections",
		"Headers",
		"Dataview",
		"Edit",
		"Metaedit"
	],
	"author": "meep.tech",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.4",
		"@types/react-dom": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"babel-jest": "^29.3.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"jest": "^29.3.1",
		"jsdom": "20.0.3",
		"obsidian": "latest",
		"obsidian-dataview": "^0.5.51",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@opd-libs/opd-metadata-lib": "0.0.2"
	}
}
